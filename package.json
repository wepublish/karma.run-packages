{
  "private": true,
  "engines": {
    "node": ">=12.0.0",
    "yarn": ">=1.17.0"
  },
  "scripts": {
    "clean": "run-p clean:**",
    "build": "run-s --print-label build:utility build:graphql build:webpack build:react build:icons build:ui build:ui-example",
    "watch": "run-p --print-label watch:*",
    "publish": "run-s --print-label publish:utility publish:graphql publish:webpack publish:react publish:ui",
    "clean:icons": "yarn workspace @karma.run/icons clean",
    "build:icons": "yarn workspace @karma.run/icons build",
    "watch:icons": "yarn workspace @karma.run/icons watch",
    "download:icons": "yarn workspace @karma.run/icons download",
    "publish:icons": "yarn workspace @karma.run/icons publish --access public --no-git-tag-version",
    "clean:ui": "yarn workspace @karma.run/ui clean",
    "build:ui": "yarn workspace @karma.run/ui build",
    "watch:ui": "yarn workspace @karma.run/ui watch",
    "publish:ui": "yarn workspace @karma.run/ui publish --access public --no-git-tag-version",
    "clean:react": "yarn workspace @wepublish/karma.run-react clean",
    "build:react": "yarn workspace @wepublish/karma.run-react build",
    "watch:react": "yarn workspace @wepublish/karma.run-react watch",
    "publish:react": "yarn workspace @wepublish/karma.run-react publish --access public --no-git-tag-version",
    "clean:webpack": "yarn workspace @karma.run/webpack clean",
    "build:webpack": "yarn workspace @karma.run/webpack build",
    "watch:webpack": "yarn workspace @karma.run/webpack watch",
    "publish:webpack": "yarn workspace @karma.run/webpack publish --access public --no-git-tag-version",
    "clean:utility": "yarn workspace @karma.run/utility clean",
    "build:utility": "yarn workspace @karma.run/utility build",
    "watch:utility": "yarn workspace @karma.run/utility watch",
    "publish:utility": "yarn workspace @karma.run/utility publish --access public --no-git-tag-version",
    "clean:graphql": "yarn workspace @karma.run/graphql clean",
    "build:graphql": "yarn workspace @karma.run/graphql build",
    "watch:graphql": "yarn workspace @karma.run/graphql watch",
    "publish:graphql": "yarn workspace @karma.run/graphql publish --access public --no-git-tag-version",
    "clean:media": "yarn workspace @karma.run/media clean",
    "build:media": "yarn workspace @karma.run/media build",
    "watch:media": "yarn workspace @karma.run/media watch",
    "publish:media": "yarn workspace @karma.run/media publish --access public --no-git-tag-version",
    "clean:media-storage-local": "yarn workspace @karma.run/media-storage-local clean",
    "build:media-storage-local": "yarn workspace @karma.run/media-storage-local build",
    "watch:media-storage-local": "yarn workspace @karma.run/media-storage-local watch",
    "publish:media-storage-local": "yarn workspace @karma.run/media-storage-local publish --access public --no-git-tag-version",
    "clean:media-image-sharp": "yarn workspace @karma.run/media-image-sharp clean",
    "build:media-image-sharp": "yarn workspace @karma.run/media-image-sharp build",
    "watch:media-image-sharp": "yarn workspace @karma.run/media-image-sharp watch",
    "test:media-image-sharp": "yarn workspace @karma.run/media-image-sharp test",
    "publish:media-image-sharp": "yarn workspace @karma.run/media-image-sharp publish --access public --no-git-tag-version",
    "clean:ui-example": "yarn workspace @karma.run/ui-example clean",
    "build:ui-example": "yarn workspace @karma.run/ui-example build",
    "watch:ui-example": "yarn workspace @karma.run/ui-example watch",
    "start:ui-example": "yarn workspace @karma.run/ui-example start",
    "clean:graphql-example": "yarn workspace @karma.run/graphql-example clean",
    "build:graphql-example": "yarn workspace @karma.run/graphql-example build",
    "watch:graphql-example": "yarn workspace @karma.run/graphql-example watch",
    "start:graphql-example": "yarn workspace @karma.run/graphql-example start",
    "clean:media-example": "yarn workspace @karma.run/media-example clean",
    "build:media-example": "yarn workspace @karma.run/media-example build",
    "watch:media-example": "yarn workspace @karma.run/media-example watch",
    "start:media-example": "yarn workspace @karma.run/media-example start"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "devDependencies": {
    "@karma.run/media": "*",
    "graphql": "^14.5.8",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "husky": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.11.1",
    "webpack": "^4.35.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern 'packages/**/*.{js,ts,tsx,json}'"
    }
  }
}
