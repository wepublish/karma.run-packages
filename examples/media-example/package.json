{
  "name": "@karma.run/media-example",
  "private": true,
  "version": "0.0.0",
  "main": "./lib/index.js",
  "license": "MIT",
  "repository": "github:karmarun/karma.media",
  "scripts": {
    "start": "node ./lib/index.js",
    "watch:start": "nodemon ./index.js",
    "clean": "shx rm -rf ./lib",
