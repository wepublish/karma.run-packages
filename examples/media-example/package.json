  "dependencies": {
    "@wepublish/krp-media-server": "*",
    "@wepublish/krp-media-storage-local": "*",
    "@wepublish/krp-media-image-sharp": "*"
  },
  "devDependencies": {
    "@types/node": "^12.0.10",
    "nodemon": "^1.19.1"
  }
}
